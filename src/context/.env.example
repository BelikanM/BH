# Configuration Appwrite - Copiez ce fichier vers .env et remplissez vos valeurs
REACT_APP_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
REACT_APP_APPWRITE_PROJECT_ID=your-project-id-here
REACT_APP_APPWRITE_DATABASE_ID=your-database-id-here
REACT_APP_APPWRITE_BUCKET_ID=your-bucket-id-here

# Collections IDs
REACT_APP_USERS_COLLECTION_ID=users
REACT_APP_VIDEOS_COLLECTION_ID=videos
REACT_APP_COMMENTS_COLLECTION_ID=comments
REACT_APP_LIKES_COLLECTION_ID=likes
REACT_APP_FOLLOWS_COLLECTION_ID=follows

# Configuration optionnelle
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_ENVIRONMENT=development

